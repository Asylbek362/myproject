<script src="https://itgk.kz/site/banner.js"></script>

<script type="text/javascript">
    jQuery(document).ready(function($) {
        
        // -----------------------------------------------------------------
        // 1. Анимация выпадающего меню (DropDown Scale Effect)
        // -----------------------------------------------------------------
        $('.header__menu__ul > li.deeper.parent').hover(
            function() { 
                $(this).find('.nav-child').css('display', 'block').stop(true, true); 
            },
            function() { 
                $(this).find('.nav-child').stop(true, true).slideUp(250); 
            }
        );

        // -----------------------------------------------------------------
        // 2. Липкое меню (Sticky Header)
        // -----------------------------------------------------------------
        var $navMenu = $('.nav-menu');
        var stickyStart = 100;

        function handleStickyHeader() {
            if ($(window).scrollTop() > stickyStart) {
                $navMenu.addClass('sticky');
            } else {
                $navMenu.removeClass('sticky');
            }
        }
        
        // -----------------------------------------------------------------
        // 3. Эффект Параллакса для видео
        // -----------------------------------------------------------------
        var $videoBg = $('.video-hero__bg');
        var $videoHero = $('.video-hero');
        var scrollSpeed = 0.5;

        function handleParallax() {
            if ($videoHero.length === 0) return;

            var scrolled = $(window).scrollTop();
            var offset = $videoHero.offset().top;
            
            if (scrolled >= offset && scrolled <= offset + $videoHero.outerHeight()) {
                var distance = (scrolled - offset) * scrollSpeed;
                $videoBg.css('transform', 'translate3d(0, ' + distance + 'px, 0)');
            } else if (scrolled < offset) {
                $videoBg.css('transform', 'translate3d(0, 0, 0)');
            }
        }
        
        // -----------------------------------------------------------------
        // 4. Анимация при прокрутке (Scroll Animation)
        // -----------------------------------------------------------------
        var $animation_elements = $('.animated-element');
        var $window = $(window);

        function check_if_in_view() {
            var window_height = $window.height();
            var window_top_position = $window.scrollTop();
            var window_bottom_position = (window_top_position + window_height);

            $.each($animation_elements, function() {
                var $element = $(this);
                var element_height = $element.outerHeight();
                var element_top_position = $element.offset().top;
                
                if (element_top_position + element_height * 0.1 < window_bottom_position && element_top_position < window_bottom_position) {
                    $element.addClass('is-visible');
                }
            });
        }
        
        // Объединяем все обработчики прокрутки
        $window.on('scroll orientationchange resize', function() {
            handleStickyHeader();
            handleParallax();
            check_if_in_view();
        });
        
        // Запускаем при загрузке
        $window.trigger('scroll');
        
        // 5. Функционал изменения размера шрифта (Используется jfontsize, как в оригинале)
        $('#jfontsize-minus').on('click', function() {
             $('div, *').jfontsize('minus');
        });
        $('#jfontsize-default').on('click', function() {
             $('div, *').jfontsize('default');
        });
        $('#jfontsize-plus').on('click', function() {
             $('div, *').jfontsize('plus');
        });
    });
</script>
</body>
</html>
